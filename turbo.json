{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**",
				".next/**"
			]
		},
		"crypto-lottery#build": {
			"dependsOn": [
				"^build",
				"$BROWSER",
				"$NEXT_TELEMETRY_DISABLED",
				"$NEXT_PUBLIC_LOTTERY_CONTRACT_ADDRESS",
				"$NODE_ENV",
				"$PORT"
			],
			"outputs": [
				".next/**"
			]
		},
		"start": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**",
				".next/**"
			]
		},
		"test": {
			"dependsOn": [
				"^lint",
				"^build"
			],
			"outputs": [
				"coverage/**"
			]
		},
		"check-format": {
			"dependsOn": [
				"^build"
			]
		},
		"format": {
			"dependsOn": [
				"^build"
			]
		},
		"check-lint": {
			"dependsOn": [
				"^build"
			]
		},
		"lint": {
			"dependsOn": [
				"^build"
			],
			"outputs": []
		},
		"server": {
			"dependsOn": [
				"^build"
			],
			"outputs": []
		},
		"deploy": {
			"dependsOn": [
				"^build",
				"^test"
			]
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": [
		".env",
		".env.local",
		".env.production.local",
		".env.development.local",
		"$BROWSER",
		"$NEXT_TELEMETRY_DISABLED",
		"$NEXT_PUBLIC_LOTTERY_CONTRACT_ADDRESS",
		"$NODE_ENV",
		"$PORT"
	]
}
